import java.util.Scanner;
class cc{
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int x = s.nextInt();
        int a[] = new int[x];
        int z=0,o=0,t=0;
        for(int i=0;i<x;i++){
            a[i] = s.nextInt();
        }
        for(int i=0;i<x;i++){
            if(a[i]==0){
                z++;
            }
            else if(a[i]==1){
                o++;
            }
            else{
                t++;
            }

        }
        for(int i=0;i<z;i++){
            System.out.print("0 ");
        }
        for(int i=0;i<o;i++){
            System.out.print("1 ");
        }
        for(int i=0;i<t;i++){
            System.out.print("2 ");
        }
        }
       
       
    }
